<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!--Insert your storage account name and key in the line below. -->
      <add key="StorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=;AccountKey=" />
      <!--Insert your Key Vault credentials in the lines below. For more information about getting started with Key Vault, 
      please look at http://azure.microsoft.com/en-us/documentation/articles/key-vault-get-started/ -->
      <add key="KVClientId" value="" />
      <add key="KVClientKey" value="" />
      <add key="VaultUri" value=""/>

      <!--Optional: Uncomment the following line and specify an ID of an existing key/secret in your vault. Otherwise a secret will be created.-->
      <!--<add key="KeyID" value="[Key ID]"/>-->
    </appSettings>
</configuration>